<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<proxies>
		<proxy>
			<id>httpproxy</id>
			<active>true</active>
			<protocol>http</protocol>
			<host>proxy.sei.cmu.edu</host>
			<port>8080</port>
			<nonProxyHosts>localhost|*.sei.cmu.edu</nonProxyHosts>
		</proxy>
		<proxy>
			<id>httpsproxy</id>
			<active>true</active>
			<protocol>https</protocol>
			<host>proxy.sei.cmu.edu</host>
			<port>8080</port>
			<nonProxyHosts>localhost|*.sei.cmu.edu</nonProxyHosts>
		</proxy>
	</proxies>
	<mirrors>
		<mirror>
			<id>eclipse-osuosl</id>
			<mirrorOf>http://download.eclipse.org/</mirrorOf>
			<url>http://ftp.osuosl.org/pub/eclipse/</url>
			<layout>p2</layout>
			<mirrorOfLayouts>p2</mirrorOfLayouts>
		</mirror>
		<mirror>
			<id>osate-build-jericho</id>
			<mirrorOf>http://osate-build.sei.cmu.edu/</mirrorOf>
			<url>http://jericho.sei.cmu.edu/</url>
			<layout>p2</layout>
			<mirrorOfLayouts>p2</mirrorOfLayouts>
		</mirror>
	</mirrors>
	<profiles>
		<profile>
			<id>allow-snapshots</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>snapshots-repo</id>
					<url>https://oss.sonatype.org/content/repositories/snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>
	</profiles>
</settings>
