<?xml version="1.0" encoding="ASCII"?>
<safe2:SystemOverview xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aadl2="http://aadl.info/AADL/2.0" xmlns:safe2="http://www.example.org/safe2">
  <fundamentals xsi:type="safe2:AccidentLevel" name="DeathOrInjury" description="A human is killed or seriously injured">
    <accident name="PatientHarmed" description="The patient is killed or seriously injured">
      <hazard name="BadInfoDisplayed" description="Incorrect information is sent to the display" hazardousFactor="SpO2 Information">
        <constraint name="ShowGoodInfo" description="The app must accurately inform the display of the status of the patient's vital signs">
          <errorType href="aadl/PulseOx_Forwarding_Errors.aadl#emv2$PulseOx_Forwarding_Errors.MissedAlarm"/>
        </constraint>
        <environmentElement href="aadl/PulseOx_Forwarding_System.aadl#/0/@ownedPublicSection/@ownedClassifier.1/@ownedAbstractSubcomponent.0"/>
        <systemElement xsi:type="aadl2:ProcessSubcomponent" href="aadl/PulseOx_Forwarding_System.aadl#/0/@ownedPublicSection/@ownedClassifier.1/@ownedProcessSubcomponent.0"/>
      </hazard>
      <hazard name="InfoLate" description="Information that is out of date is sent to the display." hazardousFactor="SpO2 Information">
        <constraint name="ShowInfoOnTime" description="The app must inform the display of the status of the patient's vital signs in a timely manner">
          <errorType href="aadl/PulseOx_Forwarding_Errors.aadl#emv2$PulseOx_Forwarding_Errors.LateAlarm"/>
        </constraint>
        <environmentElement href="aadl/PulseOx_Forwarding_System.aadl#/0/@ownedPublicSection/@ownedClassifier.1/@ownedAbstractSubcomponent.0"/>
        <systemElement xsi:type="aadl2:ProcessSubcomponent" href="aadl/PulseOx_Forwarding_System.aadl#/0/@ownedPublicSection/@ownedClassifier.1/@ownedProcessSubcomponent.0"/>
      </hazard>
    </accident>
  </fundamentals>
</safe2:SystemOverview>
