<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="org.osate.asap.view.Activator"
               id="org.osate.asap.view"
	       name="view">
    </component>
  </extension>
  <extension
        point="org.eclipse.sirius.externalJavaAction">
     <javaActions
           actionClass="org.osate.asap.view.DoFocusAction"
           id="org.osate.asap.view.DoFocusActionID">
     </javaActions>
     <javaActions
           actionClass="org.osate.asap.view.ForceVariableInitializationAction"
           id="org.osate.asap.view.ForceVariableInitializationActionID">
     </javaActions>
     <javaActions
           actionClass="org.osate.asap.view.ClearFocusAction"
           id="org.osate.asap.view.ClearFocusActionID">
     </javaActions>
  </extension>
</plugin>
